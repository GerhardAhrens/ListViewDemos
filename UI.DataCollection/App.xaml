<Application
    x:Class="UI.DataCollection.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:UI.DataCollection"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Style\ListViewStyle.xaml" />
                <ResourceDictionary Source="Resources\Style\MenuItemStyle.xaml" />
                <ResourceDictionary Source="Resources\Style\Icons.xaml" />
                
                <ResourceDictionary Source="pack://application:,,,/EasyPrototypingNET;component/Themes/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
